{
    "name": "modaq",
    "version": "1.8.2",
    "description": "Quiz Bowl Reader using TypeScript, React, and MobX",
    "repository": {
        "type": "git",
        "url": "https://github.com/alopezlago/QuizBowlReader"
    },
    "author": "Alejandro Lopez-Lago",
    "license": "MIT",
    "keywords": [
        "modaq",
        "quizbowl",
        "quiz bowl"
    ],
    "files": [
        "README.md",
        "LICENSE",
        "*"
    ],
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@types/chai": "^4.2.15",
        "@types/gapi.auth2": "^0.0.47",
        "@types/gapi.client.sheets": "^4.0.0",
        "@types/he": "^1.1.2",
        "@types/mocha": "^8.2.1",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vitejs/plugin-react": "^1.3.2",
        "chai": "^4.3.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-mobx": "^0.0.9",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "mocha": "^10.0.0",
        "prettier": "^2.2.1",
        "tsconfig-paths": "^3.9.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.3",
        "vite": "^2.9.9"
    },
    "dependencies": {
        "@fluentui/react": "^8.64.2",
        "he": "^1.2.0",
        "mobx": "^6.5.0",
        "mobx-react-lite": "^3.3.0",
        "mobx-sync": "^3.0.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    },
    "resolutions": {
        "@types/react": "17.0.3"
    },
    "//": "Note: to publish, run npm run prepublish, then npm publish out",
    "scripts": {
        "init": "yarn install",
        "build": "tsc",
        "lint": "eslint . --ext .ts,.tsx",
        "lintFix": "eslint . --fix --ext .ts,.tsx",
        "prepublish": "copy package.json out && copy README.md out && copy LICENSE out",
        "test": "mocha --recursive tests/**/*.ts --exit --check-leaks -r tests/TestInit.js -r ts-node/register -r tsconfig-paths/register",
        "dev": "vite --https",
        "buildDemo": "tsc && vite build --mode production",
        "serve": "vite preview --https"
    }
}

{
    "name": "modaq",
    "version": "1.5.0",
    "description": "Quiz Bowl Reader using TypeScript, React, and MobX",
    "repository": {
        "type": "git",
        "url": "https://github.com/alopezlago/QuizBowlReader"
    },
    "author": "Alejandro Lopez-Lago",
    "license": "MIT",
    "keywords": [
        "modaq",
        "quizbowl",
        "quiz bowl"
    ],
    "files": [
        "README.md",
        "LICENSE",
        "*"
    ],
    "devDependencies": {
        "@types/chai": "^4.2.15",
        "@types/gapi.auth2": "^0.0.47",
        "@types/gapi.client.sheets": "^4.0.0",
        "@types/mocha": "^8.2.1",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "chai": "^4.3.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "fork-ts-checker-webpack-plugin": "^6.1.1",
        "html-webpack-plugin": "^5.3.1",
        "mocha": "^8.3.0",
        "prettier": "^2.2.1",
        "source-map-loader": "^2.0.1",
        "tsconfig-paths": "^3.9.0",
        "ts-node": "^9.1.1",
        "ts-loader": "^8.0.18",
        "tsc-alias": "^1.3.4",
        "typescript": "^4.2.3",
        "webpack": "^5.22.0",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.1.0"
    },
    "dependencies": {
        "@fluentui/react": "^8.28.0",
        "mobx": "^6.1.7",
        "mobx-react-lite": "^3.2.0",
        "mobx-sync": "^3.0.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    },
    "resolutions": {
        "@types/react": "17.0.3"
    },
    "//": "Note: to publish, run npm run prepublish, then npm publish out",
    "scripts": {
        "init": "yarn install",
        "buildDemo": "webpack --mode development --watch",
        "buildDemoRelease": "webpack --mode production",
        "build": " tsc && tsc-alias",
        "lint": "eslint . --ext .ts,.tsx",
        "prepublish": "copy package.json out && copy README.md out && copy LICENSE out",
        "start": "webpack serve --color --config webpack.config.js --mode development",
        "test": "mocha --recursive tests/**/*.ts --exit --check-leaks -r tests/TestInit.js -r ts-node/register -r tsconfig-paths/register"
    }
}
